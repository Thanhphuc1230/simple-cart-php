{
    "name": "thanhphuc1230/simple-cart-php",
    "description": "A simple shopping cart package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thanh Phuc",
            "email": "thanhphuc15052002@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/session": "^8.0|^9.0",
        "illuminate/events": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Thanhphuc1230\\ShoppingCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thanhphuc1230\\ShoppingCart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thanhphuc1230\\ShoppingCart\\ShoppingCartServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 